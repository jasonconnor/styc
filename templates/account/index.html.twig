{% extends 'inc/base.html.twig' %}

{% block title %}Account{% endblock %}

{% block main %}

    <h1>Welcome, {{ app.user.username }}</h1>

    <h2>Manage Your Account</h2>

    <h3>Scores</h3>

    <ul>
        {% for score in app.user.scores %}
            <li>{{ score.score }}</li>
        {% endfor %}
    </ul>

{% endblock %}
